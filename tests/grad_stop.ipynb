   "source": [
    "tf.reset_default_graph()\n",
    "x = tf.constant([3.0])\n",
    "y = tf.constant([4.0])\n",
    "z= x*y + tf.stop_gradient(x**2)\n",
    "dz_dx = tf.gradients(z,x)[0]\n",
    "with tf.Session() as sess:\n",
    "    sess.run(tf.global_variables_initializer())\n",
    "    sess.run(z)\n",
    "    sess.run(dz_dx)"
   ]
